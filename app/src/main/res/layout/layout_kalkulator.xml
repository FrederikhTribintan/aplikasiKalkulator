<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">
    <EditText
        android:id="@+id/fbTempatAngka"
        android:layout_width="387dp"
        android:layout_height="71dp"
        android:layout_marginTop="128dp"
        android:ems="10"
        android:hint="0"
        android:inputType="text"
        android:textAlignment="textEnd"
        android:textSize="48sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/fbAngka7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="15dp"
        android:layout_marginTop="30dp"
        android:text="7"
        android:textSize="20sp"
        android:textStyle="bold"
        app:cornerRadius="0sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/fbTempatAngka" />

    <Button
        android:id="@+id/fbAngka8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="13dp"
        android:layout_marginTop="30dp"
        android:text="8"
        android:textSize="20sp"
        android:textStyle="bold"
        app:cornerRadius="0sp"
        app:layout_constraintStart_toEndOf="@+id/fbAngka7"
        app:layout_constraintTop_toBottomOf="@+id/fbTempatAngka" />

    <Button
        android:id="@+id/fbAngka9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="13dp"
        android:layout_marginTop="30dp"
        android:text="9"
        android:textSize="20sp"
        android:textStyle="bold"
        app:cornerRadius="0sp"
        app:layout_constraintStart_toEndOf="@+id/fbAngka8"
        app:layout_constraintTop_toBottomOf="@+id/fbTempatAngka" />

    <Button
        android:id="@+id/fbTambah"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="13dp"
        android:layout_marginTop="30dp"
        android:layout_marginEnd="12dp"
        android:text="+"
        android:textSize="20sp"
        android:textStyle="bold"
        app:cornerRadius="0sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/fbAngka9"
        app:layout_constraintTop_toBottomOf="@+id/fbTempatAngka" />

    <Button
        android:id="@+id/fbAngka4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="15dp"
        android:layout_marginTop="20dp"
        android:text="4"
        android:textSize="20sp"
        android:textStyle="bold"
        app:cornerRadius="0sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/fbAngka7" />

    <Button
        android:id="@+id/fbAngka5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="13dp"
        android:layout_marginTop="20dp"
        android:text="5"
        android:textSize="20sp"
        android:textStyle="bold"
        app:cornerRadius="0sp"
        app:layout_constraintStart_toEndOf="@+id/fbAngka4"
        app:layout_constraintTop_toBottomOf="@+id/fbAngka8" />

    <Button
        android:id="@+id/fbAngka6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="13dp"
        android:layout_marginTop="20dp"
        android:text="6"
        android:textSize="20sp"
        android:textStyle="bold"
        app:cornerRadius="0sp"
        app:layout_constraintStart_toEndOf="@+id/fbAngka5"
        app:layout_constraintTop_toBottomOf="@+id/fbAngka9" />

    <Button
        android:id="@+id/fbKurang"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="13dp"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="12dp"
        android:text="-"
        android:textSize="20sp"
        android:textStyle="bold"
        app:cornerRadius="0sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/fbAngka6"
        app:layout_constraintTop_toBottomOf="@+id/fbTambah" />

    <Button
        android:id="@+id/fbAngka1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="15dp"
        android:layout_marginTop="20dp"
        android:text="1"
        android:textSize="20sp"
        android:textStyle="bold"
        app:cornerRadius="0sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/fbAngka4" />

    <Button
        android:id="@+id/fbAngka2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="13dp"
        android:layout_marginTop="20dp"
        android:text="2"
        android:textSize="20sp"
        android:textStyle="bold"
        app:cornerRadius="0sp"
        app:layout_constraintStart_toEndOf="@+id/fbAngka1"
        app:layout_constraintTop_toBottomOf="@+id/fbAngka5" />

    <Button
        android:id="@+id/fbAngka3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="13dp"
        android:layout_marginTop="20dp"
        android:text="3"
        android:textSize="20sp"
        android:textStyle="bold"
        app:cornerRadius="0sp"
        app:layout_constraintStart_toEndOf="@+id/fbAngka2"
        app:layout_constraintTop_toBottomOf="@+id/fbAngka6" />

    <Button
        android:id="@+id/fbPerkalian"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="13dp"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="12dp"
        android:text="*"
        android:textSize="20sp"
        android:textStyle="bold"
        app:cornerRadius="0sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/fbAngka3"
        app:layout_constraintTop_toBottomOf="@+id/fbKurang" />

    <Button
        android:id="@+id/fbHasil"
        android:layout_width="388dp"
        android:layout_height="48dp"
        android:layout_marginStart="13dp"
        android:layout_marginTop="55dp"
        android:layout_marginEnd="10dp"
        android:text="="
        android:textSize="20sp"
        android:textStyle="bold"
        app:cornerRadius="0sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/fbTitik" />

    <TextView
        android:id="@+id/fbNIM"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="112dp"
        android:layout_marginBottom="16dp"
        android:text="2281058"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/fbHasil"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/fbNama"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="112dp"
        android:layout_marginEnd="10dp"
        android:layout_marginBottom="16dp"
        android:text="Frederikh Tribintan Ringo"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/fbHasil"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/fbTitik"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="15dp"
        android:layout_marginTop="20dp"
        android:text="."
        android:textSize="20sp"
        android:textStyle="bold"
        app:cornerRadius="0sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/fbAngka1" />

    <Button
        android:id="@+id/fbAngka0"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="13dp"
        android:layout_marginTop="20dp"
        android:text="0"
        android:textSize="20sp"
        android:textStyle="bold"
        app:cornerRadius="0sp"
        app:layout_constraintStart_toEndOf="@+id/fbTitik"
        app:layout_constraintTop_toBottomOf="@+id/fbAngka2" />

    <Button
        android:id="@+id/fbTombolHapus"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="13dp"
        android:layout_marginTop="20dp"
        android:text="C"
        android:textSize="20sp"
        android:textStyle="bold"
        app:cornerRadius="0sp"
        app:layout_constraintStart_toEndOf="@+id/fbAngka0"
        app:layout_constraintTop_toBottomOf="@+id/fbAngka3" />

    <Button
        android:id="@+id/fbPembagian"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="13dp"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="12dp"
        android:text="/"
        android:textSize="20sp"
        android:textStyle="bold"
        app:cornerRadius="0sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/fbTombolHapus"
        app:layout_constraintTop_toBottomOf="@+id/fbPerkalian" />
</androidx.constraintlayout.widget.ConstraintLayout>